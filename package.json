{
  "name": "html5parser",
  "description": "A fast, accurate AST parser for HTML5",
  "version": "1.1.2",
  "author": "acrazing <joking.young@gmail.com>",
  "keywords": [
    "html5",
    "parser",
    "ast",
    "attributes"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "assets/",
    "lib/",
    "src/"
  ],
  "scripts": {
    "build": "npx tsc",
    "clean": "rm -rf esm cjs lib temp .cache",
    "test": "jest",
    "denoify": "npx denoify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acrazing/html5parser.git"
  },
  "bugs": {
    "url": "https://github.com/acrazing/html5parser/issues"
  },
  "homepage": "https://github.com/acrazing/html5parser#readme",
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.4",
    "@types/node-fetch": "^2.5.4",
    "denoify": "^0.2.9",
    "fs-extra": "^8.1.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "cacheDirectory": ".cache/jest",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "<rootDir>/test/**/*.{ts,tsx}",
      "!**/*.d.ts"
    ],
    "coverageDirectory": "temp/coverage",
    "globals": {
      "__DEV__": true,
      "ENV": {}
    },
    "testMatch": [
      "<rootDir>/test/**/*.spec.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "cliVersion": "8.8.4",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,less,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "dependencies": {
    "tslib": "^1.10.0"
  }
}
